CALL bin\Special\Functions delay,r,1500
CLS

CALL bin\IO\Print colored,black,yellow,"[",0
ECHO | SET /p ".=^!"
CALL bin\IO\Print colored,black,yellow,"]",0
CALL bin\IO\Print colored,black,red," ~",0
ECHO | SET /p .="/"
CALL bin\IO\Print colored,black,bwhite,"START_GAME",0
CALL bin\IO\Print colored,black,bwhite,".BAT",0
CALL bin\Special\Functions delay,r,500
CALL bin\IO\Print colored,black,gray," system.ACTION.EXECUTE",1
ECHO.
CALL bin\Special\Functions delay,r,500

CALL bin\Special\Functions delay,r,500

ECHO Checking boot...
CALL bin\IO\Print tty_info,bwhite,lgreen,OK,"EFI Boot",1
ECHO.

ECHO Loading SATA driver...
SET labels_list="AMD INTEL ASMedia Standard Universal"
SET drivers_list="gfi250ww h8im04ww ax3p01ww"
CALL bin\Special\Functions randomItem,label,%labels_list%
CALL bin\Special\Functions randomItem,driver,%drivers_list%
CALL bin\IO\Print tty_info,bwhite,lgreen,OK,"'",0
CALL bin\IO\Print colored,black,laqua,"%label% SATA Controller AHCI Driver",0
ECHO | SET /p ".=' in "
CALL bin\IO\Print colored,black,lblue,"%driver%.inf",1
ECHO.

ECHO Searching drives...
CALL bin\IO\Print tty_info,bwhite,lpurple,FOUND,"",0
CALL bin\Special\Functions random,hex_groups_count,3,4
FOR /L %%i IN (1,1,%hex_groups_count%) DO CALL bin\Special\Functions random,length[%%i],4,12
CALL bin\Special\Functions random,disks_count,1,3
CALL bin\Special\Functions random,boot_device_num,1,%disks_count%
FOR /L %%k IN (1,1,%disks_count%) DO (
	FOR /L %%i IN (1,1,%hex_groups_count%) DO (
		CALL bin\Special\Functions hex_rand,hexgroup,!length[%%i]!
		IF [%boot_device_num%] EQU [%%k] SET "boot_device_str=!boot_device_str!!hexgroup!-"
		ECHO | SET /p ".=!hexgroup!-"
	)
	ECHO | SET /p ".=CC, "
)
SET "boot_device_str=!boot_device_str!CC"
CALL bin\Special\Functions hex_rand,hexgroup,!length[1]!
SET corrupted_disk=%hexgroup%
CALL bin\Special\Functions hex_rand,hexgroup,!length[2]!
SET corrupted_disk=%corrupted_disk%-%hexgroup%
CALL bin\IO\Print colored black red "%corrupted_disk%-system.DRIVE.CORRUPTED" 1
CALL bin\Special\Functions hex_rand,hexgroup,!length[3]!
SET corrupted_disk=%corrupted_disk%-%hexgroup%
ECHO.

CALL bin\Special\Functions delay,r,1000
ECHO Trying to fix corrupted bytes...
CALL bin\Special\Functions delay,r,3000
CALL bin\Special\Functions random,is_fail,1,5
IF [%is_fail%] NEQ [1] (
	CALL bin\IO\Print tty_info,bwhite,lgreen,SUCCESS,"%corrupted_disk%-",0
	CALL bin\IO\Print colored,black,lyellow,"00",0
	CALL bin\IO\Print colored,black,gray," status... unusable, disabled",1
) ELSE (
	CALL bin\IO\Print tty_info,bwhite,red,FAIL,"Cannot fix corrupted databytes. Terminating...",1
	CALL bin\Special\Functions delay,r,1000
	CALL bin\Sugar\TurnOff
)
ECHO.

CALL bin\IO\Print slowecho,"efiboot loaded from device",0
ECHO Acpi(devhd0,0)/PCI(SATA0, system.DRIVE.MASTER, "!boot_device_str!")/PCI(0^|0)/SATA1
ECHO.

CALL bin\IO\Print slowecho,"Reading boot instructions",0
CALL bin\Special\Functions delay,r,1500
ECHO | SET /p ".=| "
CALL bin\IO\Print colored,black,aqua,"EFI Loader",0
CALL bin\Special\Functions random,efi_ver,0,178643,1000000
ECHO | SET /p ".=: using version "
CALL bin\IO\Print colored,black,lyellow,"%efi_ver%",0
ECHO | SET /p ".="
CALL bin\Special\Functions random,efi_num,1,15
ECHO | SET /p ".=, boot file path: ~/%boot_path%/bootimgfv%efi_num%.efi"
ECHO.
CALL bin\Special\Functions delay,r,10000

CALL bin\Special\Functions random,count,5,10
FOR /L %%i IN (1,1,%count%) DO (
	CALL bin\Special\Functions random,multiplier,1,100
	CALL bin\Special\Functions multiplyString,str,".",!multiplier!
	ECHO !str!
